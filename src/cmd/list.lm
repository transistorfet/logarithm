#
# Command Name:	list.lm
# Version:	0.1
# Package:	Core
#

$module_info = {
	'help' => [
		"Usage: list",
		"Description: Lists all the channels currently joined"
	]
};

sub do_list {
	local($irc, $msg, $privs) = @_;

	foreach (channel_list($irc->{'channels'})) {
		irc_notice($irc, $msg->{'nick'}, "$_");
	}
	return(0);
}

